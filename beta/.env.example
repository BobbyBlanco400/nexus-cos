# Nexus COS Beta Environment Configuration
# Copy this file to .env and fill in your values

# Beta Environment
VITE_ENVIRONMENT=beta
VITE_BETA_HANDSHAKE=beta-55-45-17

# API Configuration
VITE_API_BASE_URL=https://beta.nexuscos.online
VITE_BACKEND_URL=http://localhost:3000

# Feature Flags
