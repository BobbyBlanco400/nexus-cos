# Nexus COS Development Environment Configuration
# Safe for local development - no sensitive production data

# Application Environment
NODE_ENV=development
APP_NAME=Nexus COS Dev
APP_VERSION=1.0.0-dev
APP_URL=http://localhost:3000
API_URL=http://localhost:8080

# Database Configuration
POSTGRES_DB=nexus_cos_dev
POSTGRES_USER=nexus_dev
POSTGRES_PASSWORD=dev_password_123
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
DATABASE_URL=postgresql://nexus_dev:dev_password_123@localhost:5432/nexus_cos_dev

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=dev_redis_pass
REDIS_URL=redis://:dev_redis_pass@localhost:6379

# Security (Development keys - NOT for production)
JWT_SECRET=dev_jwt_secret_not_for_production_use_only
ENCRYPTION_KEY=dev_encryption_key_32_chars_long
SESSION_SECRET=dev_session_secret_for_local_only

# API Keys for V-Suite Services (Development)
V_SCREEN_API_KEY=dev_v_screen_key
V_STAGE_API_KEY=dev_v_stage_key
V_CASTER_PRO_API_KEY=dev_v_caster_pro_key
V_PROMPTER_PRO_API_KEY=dev_v_prompter_pro_key

# AI Service API Keys (Use your own for testing)
OPENAI_API_KEY=your-dev-openai-key-here
ANTHROPIC_API_KEY=your-dev-anthropic-key-here
GOOGLE_AI_API_KEY=your-dev-google-ai-key-here
STABILITY_API_KEY=your-dev-stability-key-here
ELEVENLABS_API_KEY=your-dev-elevenlabs-key-here

# Payment Processing (Test/Sandbox keys)
STRIPE_SECRET_KEY=sk_test_your-stripe-test-key-here
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-test-key-here
PAYPAL_CLIENT_ID=your-paypal-sandbox-client-id
PAYPAL_CLIENT_SECRET=your-paypal-sandbox-secret

# Social Media Integration (Development apps)
TWITCH_CLIENT_ID=your-dev-twitch-client-id
TWITCH_CLIENT_SECRET=your-dev-twitch-secret
YOUTUBE_API_KEY=your-dev-youtube-key
FACEBOOK_APP_ID=your-dev-facebook-app-id
FACEBOOK_APP_SECRET=your-dev-facebook-secret
TWITTER_API_KEY=your-dev-twitter-key
TWITTER_API_SECRET=your-dev-twitter-secret

# Streaming Configuration (Local)
RTMP_SERVER_URL=rtmp://localhost:1935/live
HLS_SERVER_URL=http://localhost:8080/hls
WEBRTC_STUN_SERVER=stun:stun.l.google.com:19302
WEBRTC_TURN_SERVER=turn:localhost:3478

# Email Configuration (Development)
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_SECURE=false
SMTP_USER=dev@localhost
SMTP_PASS=dev_password
FROM_EMAIL=dev@nexus-cos.local

# File Storage (Local)
AWS_ACCESS_KEY_ID=dev_access_key
AWS_SECRET_ACCESS_KEY=dev_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=nexus-cos-dev-bucket

# CDN Configuration (Local)
CDN_URL=http://localhost:3000/cdn
STATIC_URL=http://localhost:3000/static

# Monitoring and Analytics (Development)
GRAFANA_ADMIN_PASSWORD=dev_grafana_pass
PROMETHEUS_RETENTION=7d
LOG_LEVEL=debug
SENTRY_DSN=

# Frontend URLs (Development)
REACT_APP_API_URL=http://localhost:8080
REACT_APP_WS_URL=ws://localhost:8080
REACT_APP_CDN_URL=http://localhost:3000/cdn

# Rate Limiting (Relaxed for development)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=1000

# CORS Configuration (Permissive for development)
CORS_ORIGIN=http://localhost:3000,http://localhost:3001,http://localhost:3002,http://localhost:5173

# SSL/TLS Configuration (Not needed for development)
SSL_CERT_PATH=
SSL_KEY_PATH=

# Backup Configuration (Disabled for development)
BACKUP_SCHEDULE=
BACKUP_RETENTION_DAYS=7
BACKUP_S3_BUCKET=

# Feature Flags (All enabled for development)
ENABLE_AI_FEATURES=true
ENABLE_LIVE_STREAMING=true
ENABLE_PAYMENT_PROCESSING=false
ENABLE_SOCIAL_INTEGRATION=true
ENABLE_ANALYTICS=true

# Performance Configuration (Relaxed for development)
MAX_UPLOAD_SIZE=50MB
MAX_CONCURRENT_STREAMS=10
MAX_CONCURRENT_USERS=100

# Security Headers (Relaxed for development)
HSTS_MAX_AGE=0
CSP_POLICY=default-src 'self' 'unsafe-inline' 'unsafe-eval'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'

# Health Check Configuration
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=5
HEALTH_CHECK_RETRIES=2

# Development specific
DEBUG=true
VERBOSE_LOGGING=true
HOT_RELOAD=true
SOURCE_MAPS=true